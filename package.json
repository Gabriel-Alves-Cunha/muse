{
	"name": "muse",
	"version": "1.5.0",
	"repository": "https://github.com/Gabriel-Alves-Cunha/muse.git",
	"description": "Just an YouTube music downloader and player.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"main": "app/index.js",
	"type": "module",
	"license": "MIT",
	"private": true,
	"scripts": {
		"release:windows-only:test": "yarn add @ffmpeg-installer/win32-x64 && cross-env DEBUG=electron-builder electron-builder --publish=never --dir --windows -c.asar=false",
		"release:windows-only": "yarn add @ffmpeg-installer/win32-x64 && cross-env DEBUG=electron-builder electron-builder --publish=never --windows",
		"release:linux:test": "cross-env DEBUG=electron-builder electron-builder --publish=never --dir --linux -c.asar=false",
		"release:linux": "cross-env DEBUG=electron-builder electron-builder --publish=never --linux",
		"preview": "yarn build && cross-env NODE_ENV=production electron --trace-warnings --unhandled-rejections=strict --inspect app",
		"dev:clean": "cross-env DEBUG=muse,muse:playlists NODE_ENV=development elecrun --clean-cache --vite --preload 'preload.ts'",
		"dev": "cross-env DEBUG=muse NODE_ENV=development elecrun --vite --preload 'preload.ts'",
		"ts:check": "tsc --noEmit --pretty --extendedDiagnostics",
		"build:electron": "elecrun build --preload 'preload.ts'",
		"lint": "eslint ./src/ --ext .ts --ext .tsx --ext .js",
		"postinstall": "electron-builder install-app-deps",
		"build": "yarn build:electron && yarn build:web",
		"test": "cross-env DEBUG=muse:tests vitest",
		"coverage": "vitest run --coverage",
		"build:web": "vite build",
		"fmt": "dprint fmt"
	},
	"devDependencies": {
		"@radix-ui/react-context-menu": "^1.0.0",
		"@radix-ui/react-dialog": "^1.0.0",
		"@radix-ui/react-popover": "^1.0.0",
		"@radix-ui/react-select": "^1.0.0",
		"@stitches/react": "^1.2.8",
		"@types/debug": "^4.1.7",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/koa": "^2.13.5",
		"@types/koa-router": "^7.4.4",
		"@types/node": "^18.7.14",
		"@types/qrcode": "^1.5.0",
		"@types/react": "^18.0.18",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"@vitejs/plugin-react": "^2.0.1",
		"@vitest/ui": "^0.23.0",
		"c8": "^7.12.0",
		"cross-env": "^7.0.3",
		"debug": "^4.3.4",
		"dprint": "^0.31.1",
		"electron": "^20.1.1",
		"electron-builder": "^23.3.3",
		"electron-devtools-installer": "^3.2.0",
		"electron-run": "^2.3.1",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.31.4",
		"eslint-plugin-react-hooks": "^4.6.0",
		"happy-dom": "^6.0.4",
		"i18next": "^21.9.1",
		"i18next-browser-languagedetector": "^6.1.5",
		"js-coroutines": "^2.4.36",
		"qrcode": "^1.5.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-i18next": "^11.18.5",
		"react-icons": "^4.4.0",
		"react-spinners": "^0.13.4",
		"react-toastify": "^9.0.8",
		"react-virtuoso": "^2.17.2",
		"typescript": "^4.8.2",
		"vite": "^3.0.9",
		"vitest": "^0.23.0",
		"zustand": "4.1.1"
	},
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"electron-updater": "^5.2.1",
		"fastest-validator": "^1.15.0",
		"fluent-ffmpeg": "^2.1.2",
		"koa": "^2.13.4",
		"koa-router": "^12.0.0",
		"node-taglib-sharp": "^5.0.0",
		"sanitize-filename": "^1.6.3",
		"ytdl-core": "^4.11.2"
	},
	"cpu": [
		"x64"
	],
	"os": [
		"windows",
		"linux"
	]
}
