{
	"name": "muse",
	"version": "1.5.0",
	"repository": "https://github.com/Gabriel-Alves-Cunha/muse.git",
	"description": "Just an YouTube music downloader and player.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"main": "build/main/index.cjs",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"release:windows-only:test": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform && cross-env DEBUG=electron-builder electron-builder --publish=never --dir --windows -c.asar=false",
		"release:windows-only": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform && cross-env DEBUG=electron-builder electron-builder --publish=never --windows",
		"release:linux:test": "cross-env DEBUG=electron-builder electron-builder --publish=never --dir --linux -c.asar=false",
		"release:linux": "cross-env DEBUG=electron-builder electron-builder --publish=never --linux",
		"dev:clean": "cross-env DEBUG=muse NODE_ENV=development hmr-electron dev --clean-cache",
		"preview": "yarn build && electron --trace-warnings --inspect build/main/index.cjs",
		"lint": "eslint ./src/ --ext .ts --ext .tsx --ext .cts --ext .cjs --ext .js",
		"build": "cross-env DEBUG=muse NODE_ENV=development hmr-electron build",
		"dev": "cross-env DEBUG=muse NODE_ENV=development hmr-electron dev",
		"ts:check": "tsc --noEmit --pretty --extendedDiagnostics",
		"postinstall": "electron-builder install-app-deps",
		"test": "cross-env DEBUG=muse:tests vitest",
		"coverage": "vitest run --coverage",
		"fmt": "dprint fmt"
	},
	"devDependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@radix-ui/react-context-menu": "^2.0.1",
		"@radix-ui/react-dialog": "^1.0.2",
		"@radix-ui/react-popover": "^1.0.2",
		"@radix-ui/react-select": "^1.1.1",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/koa": "^2.13.5",
		"@types/koa-router": "^7.4.4",
		"@types/node": "^18.11.8",
		"@types/qrcode": "^1.5.0",
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vitejs/plugin-react": "^2.2.0",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"dprint": "^0.32.2",
		"electron": "^21.2.0",
		"electron-builder": "^23.6.0",
		"electron-devtools-installer": "^3.2.0",
		"electron-updater": "^5.3.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fastest-validator": "^1.15.0",
		"fluent-ffmpeg": "^2.1.2",
		"happy-dom": "^7.6.6",
		"hmr-electron": "^0.0.7",
		"i18next": "^22.0.4",
		"i18next-browser-languagedetector": "^7.0.0",
		"koa": "^2.13.4",
		"koa-router": "^12.0.0",
		"node-taglib-sharp": "^5.0.0",
		"postcss": "^8.4.18",
		"qrcode": "^1.5.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-i18next": "^12.0.0",
		"react-icons": "^4.6.0",
		"react-spinners": "^0.13.6",
		"react-toastify": "^9.0.8",
		"react-virtuoso": "^3.1.2",
		"sanitize-filename": "^1.6.3",
		"tailwindcss": "^3.2.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.2",
		"vitest": "^0.24.4",
		"ytdl-core": "^4.11.2",
		"zustand": "4.1.4"
	},
	"cpu": [
		"x64"
	],
	"os": [
		"windows",
		"linux"
	]
}
