	"description": "Just an YouTube music downloader and player.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"main": "build/main/index.cjs",
	"type": "module",
	"license": "MIT",
	"private": true,
	"scripts": {
		"release:windows-only:test": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform &&",
		"release:windows-only": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform &&",
		"release:linux:test": "",
		"release:linux": "",
		"ts:check": "tsc --noEmit --pretty --extendedDiagnostics",
		"build": "cross-env RUST_BACKTRACE=1 tauri build",
		"dev": "cross-env RUST_BACKTRACE=1 tauri dev",
		"test": "cross-env DEBUG=muse:tests vitest",
		"coverage": "vitest run --coverage",
		"fmt": "rome format . --write",
		"build:web": "vite build",
		"lint": "rome check .",
		"dev:web": "vite"
	},
