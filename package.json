{
	"name": "muse",
	"version": "0.9.6",
	"description": "Just an Youtube music downloader and player.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Gabriel-Alves-Cunha/muse.git",
		"type": "git"
	},
	"main": "app/index.js",
	"scripts": {
		"preview": "yarn build && cross-env NODE_ENV=production electron --trace-warnings --unhandled-rejections=strict --inspect app",
		"dev:clean": "cross-env DEBUG=muse,muse:playlists NODE_ENV=development elecrun --clean-cache --vite --preload 'preload.ts'",
		"dev": "cross-env DEBUG=muse NODE_ENV=development elecrun --vite --preload 'preload.ts'",
		"build": "elecrun build --preload 'preload.ts' && vite build",
		"dist:test": "yarn build && electron-builder --dir --x64 ",
		"postinstall": "electron-builder install-app-deps",
		"lint": "eslint ./ --ext .ts --ext .tsx --ext .js",
		"dist": "yarn build && electron-builder --x64",
		"test": "cross-env DEBUG=muse:tests vitest",
		"coverage": "vitest run --coverage",
		"release": "electron-builder",
		"fmt:check": "dprint check",
		"clean": "elecrun clean",
		"fmt": "dprint fmt"
	},
	"devDependencies": {
		"@faker-js/faker": "6.3.1",
		"@radix-ui/react-context-menu": "^0.1.6",
		"@radix-ui/react-dialog": "^0.1.7",
		"@radix-ui/react-popover": "^0.1.6",
		"@radix-ui/react-tooltip": "^0.1.7",
		"@stitches/react": "^1.2.8",
		"@types/debug": "^4.1.7",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/node": "^17.0.32",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.3",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"@vitejs/plugin-react": "^1.3.2",
		"@vitest/ui": "^0.12.4",
		"@welldone-software/why-did-you-render": "^7.0.1",
		"c8": "^7.11.2",
		"cross-env": "^7.0.3",
		"dprint": "^0.27.1",
		"electron": "^18.2.0",
		"electron-builder": "^23.0.3",
		"electron-devtools-installer": "^3.2.0",
		"electron-run": "^2.3.1",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.5.0",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-error-boundary": "^3.1.4",
		"react-icons": "^4.3.1",
		"react-toastify": "^9.0.1",
		"react-virtuoso": "^2.11.0",
		"readline": "^1.3.0",
		"tilg": "^0.1.1",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4",
		"vite": "^2.9.8",
		"vitest": "^0.12.4",
		"zustand": "4.0.0-rc.1"
	},
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"debug": "^4.3.4",
		"fluent-ffmpeg": "^2.1.2",
		"node-taglib-sharp": "^4.0.2",
		"sanitize-filename": "^1.6.3",
		"ytdl-core": "^4.11.0"
	}
}
