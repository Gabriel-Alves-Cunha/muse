{
	"name": "muse",
	"version": "1.5.0",
	"description": "Just an YouTube music downloader and player.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"repository": "https://github.com/Gabriel-Alves-Cunha/muse",
	"main": "build/main/index.cjs",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"release:windows-only:test": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform && cross-env DEBUG=electron-builder electron-builder --publish=never --dir --windows -c.asar=false",
		"release:windows-only": "yarn add @ffmpeg-installer/win32-x64 --ignore-platform && cross-env DEBUG=electron-builder electron-builder --publish=never --windows",
		"release:linux:test": "cross-env DEBUG=electron-builder electron-builder --publish=never --dir --linux -c.asar=false",
		"release:linux": "cross-env DEBUG=electron-builder electron-builder --publish=never --linux",
		"preview": "yarn build && electron --trace-warnings --inspect build/main/index.cjs",
		"lint:fix": "rome check . --apply && eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
		"lint": "rome check . && eslint 'src/**/*.{js,jsx,ts,tsx,json}' && tsc ",
		"build": "cross-env DEBUG=muse NODE_ENV=production hmr-electron build",
		"dev": "cross-env DEBUG=muse NODE_ENV=development hmr-electron dev",
		"postinstall": "electron-builder install-app-deps",
		"test": "cross-env DEBUG=muse:tests vitest",
		"coverage": "vitest run --coverage",
		"fmt": "rome format . --write"
	},
	"devDependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@types/fluent-ffmpeg": "^2.1.21",
		"@types/koa": "^2.13.6",
		"@types/koa-router": "^7.4.4",
		"@types/node": "^18.15.11",
		"@types/qrcode": "^1.5.0",
		"@types/react": "^18.0.35",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"@vitejs/plugin-react-swc": "^3.3.0",
		"autoprefixer": "^10.4.14",
		"cross-env": "^7.0.3",
		"electron": "^24.1.2",
		"electron-builder": "^23.6.0",
		"electron-devtools-installer": "^3.2.0",
		"electron-updater": "^5.3.0",
		"esbuild": "^0.17.17",
		"eslint": "^8.38.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fluent-ffmpeg": "^2.1.2",
		"happy-dom": "^9.7.1",
		"hmr-electron": "^0.1.8",
		"koa": "^2.14.2",
		"koa-router": "^12.0.0",
		"node-taglib-sharp": "^5.1.0",
		"postcss": "^8.4.22",
		"qrcode": "^1.5.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.3",
		"react-icons": "^4.8.0",
		"react-toastify": "^9.1.2",
		"react-virtuoso": "^4.2.1",
		"rome": "^12.0.0",
		"tailwindcss": "^3.3.1",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vite-bundle-visualizer": "^0.6.0",
		"vitest": "^0.30.1",
		"ytdl-core": "^4.11.4",
		"zustand": "^4.3.7"
	},
	"cpu": [
		"x64"
	],
	"os": [
		"linux"
	],
	"dependencies": {}
}
